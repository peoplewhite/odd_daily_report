<header>
<h1>編輯日誌</h1>
</header>

<div class="edit-diary-wrapper">
  <%= form_for @diary do |f| %>
    <div class="description-textfield">
      <%= f.label :description %>
      <%= f.text_field :description %>
    </div>
    <div class="submit-button">
      <%= f.submit "確認修改" %>
    </div>
  <% end %>
</div>


